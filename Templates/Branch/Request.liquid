{
  "method": "POST",
  "headers": {
    {%- for header in source.headers -%}
      "{{header.Key}}": "{{header.Value | replace: "\\", "\\\\" | replace: '"', '\\"' }}",
    {%- endfor %}
      "Content-Type":"application/json"
  },
  "body": {
      "AccountId": "{{ source.body.accountId }}",
      "PostalCode": "{{ source.body.postalCode }}",
      "PricingGroup": "{{ source.body.pricingGroup }}"
  }
}