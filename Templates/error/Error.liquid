{
    "statusCode": {{source.statusCode}},
    "headers": {
      {%- for header in source.headers -%}
      {{ header | as_header }},
      {%- endfor -%}
      "Content-Type": "application/json"
    },
    "body": {
        "success": false,
        "errorCode": {{ source.statusCode }},
        "errorMessage": {{ source.body.ErrorMessage }}
    }
}
