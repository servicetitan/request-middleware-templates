{
    "statusCode": {{source.statusCode}},
    "headers": {
      {%- for header in source.headers -%}
        "{{header.Key}}": "{{header.Value  | replace: "\\", "\\\\" | replace: """, "\\"" }}" {%- unless forloop.last -%},
      {%- endunless -%}
      {%- endfor %}
    },
    "body": {
        "orderNumber": "{{ source.body.OrderNumber }}",
        "success": true,
        "errorCode": 0,
        "errorMessage": ""
    }
}
