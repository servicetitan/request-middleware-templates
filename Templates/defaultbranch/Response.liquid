{
    "statusCode": {{source.statusCode}},
    "headers": {
        {%- for header in source.headers -%}
        {{ header | as_header }},
        {%- endfor -%}
        "Content-Type": "application/json"
    },
    "body": {
        "defaultBranch": {
            "branchId": "{{ source.body.BranchId }}",
            "branchName": "{{ source.body.BranchName }}",
            "pricingGroup": "{{ source.body.PricingGroup }}",
            "phoneNumber": "{{ source.body.PhoneNumber }}",
            "contact": "{{ source.body.Contact }}",
            "address": {
                "street": "{{ source.body.Street }}",
                "unit": "{{ source.body.Unit }}",
                "city": "{{ source.body.City }}",
                "state": "{{ source.body.State }}",
                "postalCode": "{{ source.body.PostalCode }}",
                "country": "{{ source.body.Country }}"
            }
        },
        "success": true,
        "errorCode": 0,
        "errorMessage": ""
    }
}
