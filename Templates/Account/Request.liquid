{
  "method": "POST",
  "headers": {
    {%- for header in source.headers -%}
      "{{header.Key}}": "{{header.Value | replace: "\\", "\\\\" | replace: '"', '\\"' }}",
    {%- endfor %}
      "Content-Type":"application/json"
  },
  "body": {
      "Account": {
        "AccountId": "{{ source.body.accountId }}",
        "PostalCode": "{{ source.body.customIdentifierFields.postalCode }}"
      }
  }
}